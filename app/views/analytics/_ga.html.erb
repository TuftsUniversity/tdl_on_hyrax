<!-- Google tag (gtag.js) -->

<% api_key = ga_api_key %>
<% unless api_key.nil? %>

<script async src='https://www.googletagmanager.com/gtag/js?id=<%= api_key %>'></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', '<%= api_key %>');
</script>
<% end %>

<%= render partial: "analytics/tag_manager_head" %>
